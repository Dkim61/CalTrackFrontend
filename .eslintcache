\n            .then((data) => {\n                this.setState({consumptions: data})\n            })\n        // this.setState({dish_id: 0, servings: 0, date: \"\", id: 0})\n        } )\n    }\n  \n\n    render() \n    {\n        return (\n            <div className=\"profile-page\">\n                <h2 className=\"username\">Welcome, {this.props.userAPI.username}</h2>\n\n\n            <div className='info-page'>\n\n                <div className='img-container'>\n                    <img src={this.props.userInfo} alt={this.props.userInfo} className=\"profile-img\" />\n                 </div>\n                \n                    \n                 <div className=\"about-me-container\">\n                        <div className='about-me-info'>\n                            <h3 className=\"about-me-title\">About Me</h3>\n                            <h4 className=\"name-title\">Name</h4>\n                            <p>{this.props.userAPI.first_name} {this.props.userAPI.last_name}</p>\n                            <h4 className=\"calorie-limit-title\">Calorie Limit</h4>\n                            <p>{this.props.userAPI.calorie_limit}</p>\n                            <h4 className=\"goal-weight-title\">Goal Weight</h4>\n                            <p>{this.props.userAPI.goal_weight} </p>\n                            <h4 className=\"weight-title\">Current Weight</h4>\n                            <p>{this.props.userAPI.weight}</p>\n                            <h4 className=\"height-title\">Height</h4>\n                            <p>{this.props.userAPI.height}</p>\n                            <h4 className=\"BMI-title\">BMI</h4>\n                            <p></p>\n                            <h4 className=\"starting-weight-title\">Starting Weight</h4>\n                            <p>{this.props.userAPI.starting_weight}</p>\n                            <h4 className=\"starting-date-title\">Starting Date</h4>\n                            <p>{this.props.userAPI.starting_date}</p>\n                            <h4 className=\"starting-weight-title\">Starting Weight</h4>\n                            <p>{this.props.userAPI.weight}</p>\n                        </div>\n                 </div>\n                 {/* <div className=\"consumption-container\">\n                    <ConsumptionsContainer dishesOptions={this.props.dishesOptions}/>\n                 </div> */}\n                  <div className=\"profile-consumption-container\">\n                    <ProfileConsumptionContainer consumptionApi={this.state.consumptions} handleDelete={this.handleDelete} consumptionUpdate={this.consumptionUpdate} dishesOptions={this.props.dishesOptions} />\n                 </div>\n            </div>\n\n\n             \n\n            </div>\n        );\n    }\n}\n\nexport default ProfilePageContainer;",["91","92"],"/Users/david/Flatiron/code/Mod5/2CalTrackFrontend/src/Containers/AddConsumptionContainer/AddConsumptionContainer.js",[],"/Users/david/Flatiron/code/Mod5/2CalTrackFrontend/src/Containers/ProfileConsumptionContainer/ProfileConsumptionContainer.js",["93"],"/Users/david/Flatiron/code/Mod5/2CalTrackFrontend/src/App.js",[],["94","95"],"/Users/david/Flatiron/code/Mod5/2CalTrackFrontend/src/Containers/LoginContainer/LoginContainer.js",[],["96","97"],"/Users/david/Flatiron/code/Mod5/2CalTrackFrontend/src/Containers/Sidebar/Sidebar.js",["98","99","100","101"],"/Users/david/Flatiron/code/Mod5/2CalTrackFrontend/src/Components/ConsumptionForm/ConsumptionForm.js",["102","103","104","105"],"/Users/david/Flatiron/code/Mod5/2CalTrackFrontend/src/Containers/ConsumptionsContainer/ConsumptionsContainer.js",[],"/Users/david/Flatiron/code/Mod5/2CalTrackFrontend/src/index.js",[],["106","107"],"/Users/david/Flatiron/code/Mod5/2CalTrackFrontend/src/Components/404/NotFound.js",[],"/Users/david/Flatiron/code/Mod5/2CalTrackFrontend/src/Containers/HomePageContainer/HomePageContainer.js",["108","109","110","111"],"/Users/david/Flatiron/code/Mod5/2CalTrackFrontend/src/C